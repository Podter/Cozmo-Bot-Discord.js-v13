{
  "name": "cozmo-bot",
  "description": "A Discord bot named \"Cozmo\" (aka. Podter's Bot)",
  "main": "index.ts",
  "scripts": {
    "build": "run-script-os",
    "build:win32": "sed \"s/typeScript: true,/typeScript: false,/\" ./index.ts >> ./index-js.ts && tsc && powershell rm index-js.ts",
    "build:darwin:linux": "sed \"s/typeScript: true,/typeScript: false,/\" ./index.ts >> ./index-js.ts && tsc && rm ./index-js.ts",
    "start": "npm run build && node dist/index-js.js",
    "dev": "nodemon index.ts",
    "heroku": "cp example.env .env && npm start"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Podter/Cozmo-Bot.git"
  },
  "author": "Podter",
  "bugs": {
    "url": "https://github.com/Podter/Cozmo-Bot/issues"
  },
  "homepage": "https://github.com/Podter/Cozmo-Bot#readme",
  "dependencies": {
    "@discord-player/extractor": "^3.0.2",
    "@discordjs/opus": "^0.7.0",
    "@discordjs/voice": "^0.8.0",
    "discord-player": "^5.2.2",
    "discord.js": "13.6.0",
    "dotenv": "^16.0.0",
    "ffmpeg-static": "^4.4.1",
    "libsodium-wrappers": "^0.7.10",
    "mongoose": "^6.2.10",
    "nodemon": "^2.0.15",
    "run-script-os": "^1.1.6",
    "ts-node": "^10.7.0",
    "typescript": "4.6",
    "wokcommands": "^1.5.3"
  },
  "engines": {
    "node": "16.14.2"
  }
}
