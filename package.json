{
  "name": "cozmo-bot",
  "description": "A Discord bot named \"Cozmo\" (aka. Podter's Bot)",
  "main": "index.ts",
  "scripts": {
    "build": "run-script-os",
    "build:win32": "sed \"s/typeScript: true,/typeScript: false,/\" ./index.ts >> ./index-js.ts && tsc && powershell rm index-js.ts",
    "build:darwin:linux": "sed \"s/typeScript: true,/typeScript: false,/\" ./index.ts >> ./index-js.ts && tsc && rm ./index-js.ts",
    "start": "npm run build && node dist/index-js.js",
    "dev": "nodemon index.ts",
    "heroku": "cp example.env .env && npm run build && rm -rd -f commands modules index.ts && node dist/index-js.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Podter/Cozmo-Bot.git"
  },
  "author": "Podter",
  "bugs": {
    "url": "https://github.com/Podter/Cozmo-Bot/issues"
  },
  "homepage": "https://github.com/Podter/Cozmo-Bot#readme",
  "dependencies": {
    "@discordjs/voice": "^0.7.5",
    "discord.js": "github:Podter/discord.js-13.3.1-fix",
    "dotenv": "^10.0.0",
    "libsodium-wrappers": "^0.7.9",
    "mongoose": "^6.2.9",
    "nodemon": "^2.0.15",
    "run-script-os": "^1.1.6",
    "spotify-to-yt": "^1.1.1",
    "ts-node": "^10.7.0",
    "typescript": "4.5",
    "wokcommands": "^1.5.3",
    "youtube-dl-exec": "^1.3.4",
    "ytdl-core": "^4.9.1",
    "ytpl": "^2.2.3"
  },
  "engines": {
    "node": "16.14.2"
  }
}
